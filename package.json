{
  "name": "filter-keys.js",
  "description": "For each element in source `object` apply `iteratee` to (key, value, object). Include key in result array where return value is `true`.",
  "version": "0.3.4",
  "author": "Wil Moore III <wil.moore@wilmoore.com>",
  "bugs": {
    "url": "https://github.com/wilmoore/filter-keys.js/issues"
  },
  "dependencies": {
    "curry2": "^1.0.3"
  },
  "devDependencies": {
    "dependency-check": "^3.0.0",
    "fixpack": "^2.2.0",
    "istanbul": "^0.4.5",
    "nodemon": "^1.12.1",
    "standard": "^11.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.0.0",
    "tape-catch": "^1.0.4"
  },
  "homepage": "https://github.com/wilmoore/filter-keys.js",
  "keywords": [
    "filter",
    "object",
    "keys"
  ],
  "license": "MIT",
  "main": "index.js",
  "preferGlobal": false,
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/wilmoore/filter-keys.js"
  },
  "scripts": {
    "cover": "istanbul cover test.js",
    "dependency-check": "dependency-check ./package.json && dependency-check ./package.json --unused --no-dev",
    "dev": "nodemon -x 'yarn run test --silent' -e 'js json'",
    "fixpack": "fixpack",
    "postversion": "git push --follow-tags",
